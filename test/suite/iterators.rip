ranges()
  arr = []
  for i in 1:5
    arr.push(i)
  arr2 = []
  for i in 1:
    arr2.push(i)
    if i == 5
      break
  Test.test("Range.get_iter()", arr.to_string(), arr2.to_string())

main()
